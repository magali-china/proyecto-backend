{{#if last_name}}
<h2>{{last_name}}</h2>
{{else}}
<h2>aca deberia ir un apellido si existe</h2>
{{/if}}
<div class="d-flex justify-content-evenly">
    {{#each alumnos}}
        <div class="card" style="width: 15rem">
            <img src="{{this.photo}}" class="card-img-top" style="width: 15rem; height: 8rem " alt="...">
            <div class="card-body">
                <h5 class="card-title">{{this.name}}</h5>
                <p class="card-text">Alumno de backend</p>
                <span class="emit_data" class="btn btn-primary">+Info </span>  
        </div>
    {{/each}}
</div>